        new Actions(wd).moveToElement(wd.findElement(By.linkText("Datawarehousing"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("QA"))).build().perform();
        wd.findElement(By.linkText("QA")).click();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.caption"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//div[@class='formfetchjobs']/div"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.caption"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Events"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.caption"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("h2"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.col-md-8"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("h2"))).build().perform();
        wd.quit();
    }
    
    public static boolean isAlertPresent(FirefoxDriver wd) {
        try {
            wd.switchTo().alert();
            return true;
        } catch (NoAlertPresentException e) {
            return false;
        }
    }
}
