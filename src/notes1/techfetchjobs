import java.util.concurrent.TimeUnit;
import java.util.Date;
import java.io.File;
import org.openqa.selenium.support.ui.Select;
import org.openqa.selenium.interactions.Actions;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.*;
import static org.openqa.selenium.OutputType.*;

public class  {
    public static void main(String[] args) throws Exception {
        FirefoxDriver wd;
        wd = new FirefoxDriver();
        wd.manage().timeouts().implicitlyWait(60, TimeUnit.SECONDS);
        wd.get("http://uat.techfetch.com/");
        new Actions(wd).moveToElement(wd.findElement(By.id("miniheader-top-first"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header/div[1]"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.main-navigation.animated"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.id("ucHeaderCtrl_notloggedin"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Home"))).build().perform();
        wd.findElement(By.linkText("Home")).click();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header/div[2]/div/div"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Home"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.id("ucHeaderCtrl_notloggedin"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("ul.home.dropdown-menu"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("FETCH Jobs"))).build().perform();
        wd.findElement(By.linkText("FETCH Jobs")).click();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.caption"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header/div[2]/div/div"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.id("ucHeaderCtrl_divJobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("FETCH Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Java J2EE"))).build().perform();
        wd.findElement(By.linkText("Java J2EE")).click();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.caption"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header/div[2]/div/div"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.id("ucHeaderCtrl_divJobs"))).build().perform();
