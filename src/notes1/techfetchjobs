        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("ul.home.dropdown-menu"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("FETCH Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Java J2EE"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Dot Net, C#"))).build().perform();
        wd.findElement(By.linkText("Dot Net, C#")).click();
        new Actions(wd).moveToElement(wd.findElement(By.cssSelector("div.caption"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.xpath("//form[@id='form1']/header"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.id("ucHeaderCtrl_notloggedin"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("FETCH Jobs"))).build().perform();
        new Actions(wd).moveToElement(wd.findElement(By.linkText("Dot Net, C#"))).build().perform();
